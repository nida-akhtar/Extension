{
    "manifest_version": 3,
    "name": "YouTube Video Summarizer",
    "version": "1.0",
    "description": "Summarize YouTube videos using AI",
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  